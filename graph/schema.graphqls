type Video {
  src: String!
  thumbnail: Image
}

type Image {
  src: String!
  alt: String
}

type Link {
  src: String!
  text: String!
}

type Page {
  id: ID!
  src: String!
  links: [Link!]!
  images: [Image!]!
  videos: [Video!]!
}

type Query {
  page(url: String!): Page
}

type Mutation {
  addPage(src: String!): Page!
  addPages(src: [String!]!): [Page!]!
}
